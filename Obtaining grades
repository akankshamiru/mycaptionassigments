#include<stdio.h>
int main()
{
	int x;
	printf("The marks obtained by the student:");
    scanf("%d",&x);
    printf("Grade obtained by the student is:");
	if(x>=85 && x<=100)
	{
	printf("Grade 'A'");
    }
    	else if(x>=70 && x<=84)
	{
	printf("Grade 'B'");
    }
    	else if(x>=55 && x<=69)
	{
	printf("Grade 'C'");
    }
    	else if(x>=40 && x<=54)
	{
	printf("Grade 'D'");
    }
    	else if(x<=40)
	{
	printf("Grade 'F'");
    }
    
}
	
