#include<stdio.h>
int main()
{
	int x;
	printf("Enter the number:");
	scanf("%d",&x);
	switch(x)
	{
		case(1):
			printf("Food item : Pizza\n");
			printf("Prize : Rs 239");
			break;
		case(2):
			printf("Food item : Burger\n");
			printf("Prize : Rs 129");
			break;
		case(3):
			printf("Food item : Pasta\n");
			printf("Prize : Rs 179");
			break;
		case(4):
			printf("Food item : French Fries\n");
			printf("Prize : Rs 99");
			break;
		case(5):
			printf("Food item : Sandwich\n");
			printf("Prize : Rs 149");
			break;
	        default:
			printf("Not available");
	}
}
